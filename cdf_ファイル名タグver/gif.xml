<?xml version="1.0" encoding="Shift_JIS" ?>
<converter version="1">
	<section>
		<type>Executable</type>
		<file>gif2png.exe</file>
		<extension>gif</extension>
		<decode>
			<initialize></initialize>
			<command>-p <sourcefile /></command>
			<finalize>mv <sourcefile />png <targetdir /></finalize>
		</decode>
	</section>
</converter>
