<?xml version="1.0" encoding="Shift_JIS" ?>
<converter version="1">
	<section type="Common Archiver">
		<file>Tar32.DLL</file>
		<function>Tar</function>
		<extension>tar</extension>
		<extension>gz</extension>
		<icon>tar.ico</icon>
		<wildcard>*.*</wildcard>
		<decode>
			<command>-x --use-directory=1 -v -f %s %s %s</command>
		</decode>
	</section>
</converter>
